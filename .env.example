# Frontend variables (Vite requires VITE_ prefix for client-side vars)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Server variables
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_KEY=your_supabase_service_role_key
PORT=4010
VITE_PORT=4011

# Database password (get from Supabase Dashboard > Settings > Database)
# Required for running migrations with: supabase db push -p $SUPABASE_DB_PASSWORD
SUPABASE_DB_PASSWORD=your_database_password