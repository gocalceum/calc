# Supabase Configuration
# Frontend variables (Vite requires VITE_ prefix for client-side vars)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Server variables
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key
SUPABASE_DB_PASSWORD=your-database-password

# OAuth Configuration (Public client IDs)
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_APPLE_CLIENT_ID=your-apple-client-id

# Application Configuration
NODE_ENV=development
VITE_APP_ENV=development
PORT=4010
VITE_PORT=4011

# API Configuration
VITE_API_TIMEOUT=30000
VITE_API_RETRY_COUNT=3

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_SENTRY=false
VITE_ENABLE_DEBUG=true

# Server Configuration (if using Bun server)
FRONTEND_URL=http://localhost:4011
CORS_ORIGINS=http://localhost:4011,http://localhost:4010

# Email Service (Resend)
RESEND_API_KEY=your-resend-api-key

# AWS Configuration (for deployment scripts)
AWS_REGION=eu-west-2